{
  "name": "@loopstack/markdown-parser",
  "version": "1.0.4",
  "description": "A markdown parser that extracts structured data according to JSON schemas",
  "author": {
    "name": "Jakob Klippel",
    "url": "https://www.linkedin.com/in/jakob-klippel-414a30261/"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublish": "npm run build",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "lint": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "keywords": [
    "markdown",
    "parser",
    "schema",
    "json-schema"
  ],
  "license": "MIT",
  "dependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/mdast": "^4.0.4",
    "@types/unist": "^3.0.3",
    "ajv": "^8.12.0",
    "lodash-es": "^4.17.21",
    "remark-parse": "^10.0.2",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.4.2",
    "typescript": "^5.1.6",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=14.16"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
